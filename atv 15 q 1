#include <stdio.h>
#include <stdlib.h>
#include <string.h>

struct horario{
  int hora;
  int min;
  int seg;
};

struct data{
  int dia;
  int mes;
  int ano;
};

struct compromisso{
  char data[10];
  char horario[10];
  char texto[50];
};

int main(void) {
  struct horario hms;
  struct data dma;
  struct compromisso dhc;
  
  return 0;
}
